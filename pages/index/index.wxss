.index_swpier swpier {
  width: 750rpx;
  height: 340rpx;
}
.index_swpier swpier image {
  width: 750rpx;
}
.index_cat {
  display: flex;
}
.index_cat navigator {
  flex: 1;
  padding: 20rpx;
}
.index_cat navigator .index_cat_view {
  width: 100rpx;
  font-size: 50rpx;
  text-align: center;
}
.index_cat navigator .index_cat_view image {
  width: 100%;
}
.index_floor .index_floor_group .index_floor_title image {
  width: 100%;
}
.index_floor .index_floor_group .index_floor_content {
  overflow: hidden;
}
.index_floor .index_floor_group .index_floor_content navigator {
  float: left;
  width: 32%;
}
.index_floor .index_floor_group .index_floor_content navigator:nth-last-child(-n+4) {
  height: 15.93962264vw;
  border-left: 10rpx solid #fff;
}
.index_floor .index_floor_group .index_floor_content navigator:nth-child(2),
.index_floor .index_floor_group .index_floor_content navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .index_floor_group .index_floor_content navigator image {
  width: 100%;
  height: 100%;
}

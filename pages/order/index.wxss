.order_main .order_item {
  border-bottom: 1px solid #ccc;
  padding: 20rpx;
  color: #666;
}
.order_main .order_item .order_num_row {
  display: flex;
  justify-content: space-between;
}
.order_main .order_item .order_price_row {
  display: flex;
  justify-content: space-between;
  padding: 10rpx 0;
}
.order_main .order_item .order_price_row .order_price_content {
  color: var(--themecolor);
}
.order_main .order_item .order_time_row {
  display: flex;
  justify-content: space-between;
}
.order_main .order_item .order_time_row .order_time_content {
  color: black;
}

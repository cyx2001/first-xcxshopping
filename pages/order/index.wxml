<Tabs tabs="{{tabs}}" binditemchange="handleItemchange">
	<view class="order_main">
		<view class="order_item" wx:for="{{orderlist}}" wx:key="order_id">
			<view class="order_num_row">
				<view class="order_num_text">订单编号</view>
				<view class="order_num_content">{{item.order_number}}</view>
			</view>
			<view class="order_price_row">
				<view class="order_price_text">订单价格</view>
				<view class="order_price_content">￥{{item.order_price}}</view>
			</view>
			<view class="order_time_row">
				<view class="order_time_text">创建时间</view>
				<view class="order_time_content">{{item.create_time}}</view>
			</view>
		</view>
	</view>
</Tabs>